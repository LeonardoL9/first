import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        int a,b,c,d;
        a=sc.nextInt();
        b=sc.nextInt();
        c=sc.nextInt();
        d=sc.nextInt();
        int h,s;
        s=(d+60-b)%60;
        h=d-b<0?c-a-1:c-a;
        System.out.println(h+" "+s);
    }
}
